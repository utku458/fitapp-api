{
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_DB_SERVER;Port=3306;Database=YOUR_DB_NAME;User Id=YOUR_DB_USER;Password=YOUR_DB_PASSWORD;"
  },
 "JwtSettings": {
  "SecretKey": "YOUR_JWT_SECRET_KEY_HERE",
  "Issuer": "FitApp"
  },
  "AWS": {
    "Region": "eu-north-1",
    "AccessKey": "YOUR_AWS_ACCESS_KEY",
    "SecretKey": "YOUR_AWS_SECRET_KEY"
  },
  "Google": {
    "ClientId": "YOUR_GOOGLE_CLIENT_ID"
  },
 "EmailSettings": {
    "AccessKey": "YOUR_AWS_ACCESS_KEY",
    "SecretKey": "YOUR_AWS_SECRET_KEY",
    "Region": "eu-north-1",
    "FromEmail": "YOUR_EMAIL@example.com"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}

